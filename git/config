[user]
	name = Cypress Frankenfeld
	email = iamcypress@gmail.com
[push]
	default = simple
[core]
	editor = vim
	excludesfile = ~/.config/git/ignore
	autocrlf = input
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[alias]
	lg = log --graph --abbrev-commit --decorate --format=format:'%C(dim blue)%h%C(reset) %C(dim green)%<(12,trunc)%ar%C(reset) %w(,2,4) %C(normal)%s%C(reset) %C(bold yellow)%d%C(reset)'
[pull]
	rebase = false
[init]
	defaultBranch = main
